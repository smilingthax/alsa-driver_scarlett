srcdir=@srcdir@
VPATH=@srcdir@
prefix=@prefix@
moddir=@moddir@
c_opts=@c_opts@
m_opts=@m_opts@
processor=@processor@
msmp=@msmp@
CONFIG_SND_VERSION=@CONFIG_SND_VERSION@
kaversion=@kaversion@
kversion=@kversion@
kpatchlevel=@kpatchlevel@
ksublevel=@ksublevel@
kextraversion=@kextraversion@
GENKSYMS=@GENKSYMS@

CONFIG_SND_KERNELDIR=@CONFIG_SND_KERNELDIR@
CONFIG_SND_MVERSION=@CONFIG_SND_MVERSION@
CONFIG_SND_PCI=@CONFIG_SND_PCI@
CONFIG_ISAPNP_KERNEL=@CONFIG_ISAPNP_KERNEL@
CONFIG_ISAPNP=@CONFIG_ISAPNP@
CONFIG_ISAPNP_NEW=0
CONFIG_SND_SGI=@CONFIG_SND_SGI@
CONFIG_SND_SEQUENCER=@CONFIG_SND_SEQUENCER@
CONFIG_SND_OSSEMUL=@CONFIG_SND_OSSEMUL@

CC=@CC@ $(CFLAGS) $(CPPFLAGS) -DALSA_BUILD -D__KERNEL__
CPP=@CPP@ $(CPPFLAGS) -M -D__DEPEND__ -DALSA_BUILD -D__KERNEL__
INCLUDE=-I$(CONFIG_SND_KERNELDIR)/include -I$(TOPDIR)
COPTS=$(c_opts) -Wall -Wstrict-prototypes -fomit-frame-pointer -pipe
LINKER=ld -r
MAKE+=$(m_opts)
SNDVERSIONS=$(TOPDIR)/include/sndversions.h
DEXPORT=$(TOPDIR)/include/modules
