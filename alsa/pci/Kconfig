config SND_PDPLUS
	tristate "Sek'D/Marian Prodif Plus"
	depends on SND
	select SND_PCM
	help
	  Say 'Y' or 'M' to include support for Sek'D/Marian Prodif Plus
          soundcard.

config SND_HDSPM
	tristate "RME Hammerfall DSP MADI"
	depends on SND
	select SND_HWDEP
	select SND_PCM
	help
	  Say 'Y' or 'M' to include support for RME Hammerfall DSP MADI
	  soundcards.

