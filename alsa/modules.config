#
# Module dependencies
#
# Module is specified without extension.
# Final (soundcard) modules have prefix '|'.
#


%DIR kernel
persist
snd persist
snd-mixer snd
snd-pcm snd
snd-midi snd
snd-timer snd
snd-hwdep snd

%DIR kernel/pcm1
snd-pcm1 snd-pcm snd-timer

%DIR kernel/seq
snd-seq snd snd-timer
snd-seq-device snd
snd-seq-midi snd-seq snd-seq-device
snd-seq-midi-emul snd-seq
snd-seq-instr snd-seq

%DIR kernel/seq/instr
snd-ainstr-simple snd-seq-instr
snd-ainstr-gf1 snd-seq-instr
snd-ainstr-iw snd-seq-instr

%DIR detect
snd-detect snd

%DIR lowlevel/generic
snd-mpu401-uart snd-midi snd-seq-midi
snd-opl3 snd-hwdep snd-timer
snd-ac97-codec snd-mixer
snd-ak4531-codec snd-mixer
snd-uart16550 snd-midi snd-seq

%DIR lowlevel/gus
snd-gus snd-pcm1 snd-mixer snd-timer snd-midi snd-seq-midi snd-seq-device
snd-synth-gus snd-seq-midi-emul snd-ainstr-iw snd-ainstr-gf1 snd-ainstr-simple

%DIR lowlevel/i2c
snd-i2c snd
snd-tea6330t snd-i2c snd-mixer

%DIR lowlevel/isa
snd-ad1848 snd-pcm1 snd-mixer
snd-cs4231 snd-pcm1 snd-mixer snd-timer
snd-es1688 snd-pcm1 snd-mixer
snd-es18xx snd-pcm1 snd-mixer
snd-cs4236 snd-cs4231 snd-pcm1 snd-mixer
snd-wavefront-synth snd-hwdep snd-seq-midi
snd-wavefront-fx snd-hwdep

%DIR lowlevel/pci
snd-s3-86c617 snd-pcm1 snd-mixer
snd-ens1370 snd-pcm1 snd-ak4531-codec snd-mixer snd-midi snd-seq-midi
snd-ens1371 snd-pcm1 snd-ac97-codec snd-mixer snd-midi snd-seq-midi
snd-es1938 snd-pcm1 snd-mixer
snd-trident_dx_nx snd-pcm1 snd-ac97-codec
snd-cs461x snd-pcm1 snd-ac97-codec
snd-fm801 snd-pcm1 snd-ac97-codec

%DIR lowlevel/sb
snd-sb8-dsp snd-pcm1 snd-mixer snd-midi snd-seq-midi
snd-sb16-dsp snd-pcm1 snd-mixer
snd-sb16-csp snd-hwdep
snd-synth-emu8000 snd-seq-device snd-seq-midi-emul

%dir lowlevel/hal2
snd-hal2 snd-pcm1

%DIR cards

|snd-card-dummy		"Dummy (/dev/null) soundcard" \
			snd-mixer snd-pcm1

|snd-card-interwave	"ISA: InterWave / Gravis UltraSound PnP" \
			snd-gus snd-synth-gus snd-cs4231

|snd-card-interwave-stb "ISA: InterWave + TEA6330T (UltraSound 32-Pro)" \
                        snd-gus snd-synth-gus snd-cs4231 snd-tea6330t

|snd-card-gusmax	"ISA: Gravis UltraSound MAX" \
			snd-gus snd-synth-gus snd-cs4231

|snd-card-gusextreme	"ISA: Gravis UltraSound Extreme" \
			snd-gus snd-synth-gus snd-es1688

|snd-card-gusclassic	"ISA: Gravis UltraSound Classic" \
			snd-gus snd-synth-gus

|snd-card-es1688	"ISA: ESS ES1688" \
			snd-es1688 snd-mpu401-uart snd-opl3

|snd-card-es18xx	"ISA: ESS ES18XX" \
			snd-es18xx snd-mpu401-uart snd-opl3

|snd-card-sb8		"ISA: Sound Blaster 1.0/2.0/Pro" \
			snd-sb8-dsp snd-opl3

|snd-card-sb16		"ISA: Sound Blaster 16" \
			snd-sb16-dsp snd-sb16-csp snd-mpu401-uart snd-opl3

|snd-card-sbawe		"ISA: Sound Blaster AWE 32/64" \
			snd-sb16-dsp snd-sb16-csp snd-mpu401-uart snd-opl3 \
			snd-synth-emu8000

|snd-card-opl3sa2	"ISA: Yamaha OPL3-SA2/SA3" \
			snd-cs4231 snd-mpu401-uart snd-opl3

|snd-card-mozart	"ISA: Mozart" \
			snd-ad1848

|snd-card-sonicvibes	"PCI: S3 SonicVibes" \
			snd-s3-86c617 snd-mpu401-uart snd-opl3

|snd-card-ens1370	"PCI: (Creative) Ensoniq AudioPCI 1370" \
			snd-ens1370

|snd-card-ens1371	"PCI: (Creative) Ensoniq AudioPCI 1371" \
			snd-ens1371

|snd-card-ad1848	"ISA: Generic AD1848/CS4248" \
			snd-ad1848

|snd-card-als100	"ISA: Avance Logic ALS100" \
			snd-sb16-dsp snd-mpu401-uart snd-opl3

|snd-card-azt2320	"ISA: Aztech Systems AZT2320" \
			snd-sb8-dsp snd-mpu401-uart snd-opl3

|snd-card-cs4231	"ISA: Cirrus Logic CS4231" \
			snd-cs4231 snd-mpu401-uart

|snd-card-cs4232	"ISA: Cirrus Logic CS4232" \
			snd-cs4231 snd-mpu401-uart snd-opl3

|snd-card-cs4236	"ISA: Cirrus Logic CS4236" \
			snd-cs4236 snd-cs4231 snd-mpu401-uart snd-opl3

|snd-card-cs461x	"PCI: Cirrus Logic (Sound Fusion) CS4280/CS461x" \
			snd-cs461x

|snd-card-es968		"ISA: ESS ES968" \
			snd-sb8-dsp

|snd-card-fm801		"PCI: ForteMedia FM801" \
			snd-fm801 snd-mpu401-uart snd-opl3

|snd-card-es1938	"PCI: ESS ES1938/1946 (Solo-1)" \
			snd-es1938 snd-opl3

|snd-card-opti9xx	"ISA: Opti9xx" \
			snd-cs4231 snd-ad1848 snd-mpu401-uart snd-opl3

|snd-card-serial	"ISA: UART16550 - MIDI only driver" \
			snd-uart16550

|snd-card-trid4dwave	"PCI: Trident 4D-Wave DX/NX" \
			snd-trident_dx_nx snd-ac97-codec snd-mpu401-uart

|snd-card-sgalaxy	"ISA: Aztech Sound Galaxy" \
			snd-ad1848

|snd-card-wavefront	"ISA: Turtle Beach Maui,Tropez,Tropez+ (Wavefront)" \
			snd-cs4231 snd-mpu401-uart snd-opl3 \
			snd-wavefront-synth snd-wavefront-fx

|snd-card-hal2		"SGI: HAL2" \
			snd-hal2

|snd-card-cmi8330	"ISA: C-Media CMI8330" \
			snd-ad1848 snd-sb16-dsp
